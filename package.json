{
  "name": "astrobench-cli",
  "description": "JavaScript benchmarks in the web browser using Benchmark.js and Puppeteer",
  "version": "0.0.0-development",
  "author": {
    "name": "Ferdinand Prantl",
    "email": "prantlf@gmail.com",
    "url": "http://prantl.tk"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/prantlf/astrobench-cli/blob/master/LICENSE"
    }
  ],
  "homepage": "https://github.com/prantlf/astrobench-cli#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/prantlf/astrobench-cli.git"
  },
  "bugs": {
    "url": "https://github.com/prantlf/astrobench-cli/issues"
  },
  "preferGlobal": true,
  "bin": {
    "astrobench": "./bin/astrobench"
  },
  "main": "lib/runner.js",
  "files": [
    "bin",
    "lib"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "lint": "eslint --fix bin/* lib/*.js 'test/**/*.js'",
    "check": "nyc --silent node test/text-formatter && nyc --silent --no-clean node test/json-formatter && nyc --silent --no-clean node test/runner && nyc --silent --no-clean ./bin/astrobench && nyc report",
    "test": "npm run lint && npm run check"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "keywords": [
    "benchmark",
    "puppeteer"
  ],
  "dependencies": {
    "chalk": "3.0.0",
    "commander": "4.1.0",
    "connect": "3.7.0",
    "fs-extra": "8.1.0",
    "puppeteer": "2.1.0",
    "puppeteer-firefox": "0.5.1",
    "serve-static": "1.14.1"
  },
  "devDependencies": {
    "@prantlf/astrobench": "1.1.0",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "nyc": "15.0.0",
    "test": "0.6.0"
  }
}
